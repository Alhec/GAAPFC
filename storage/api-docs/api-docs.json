{
    "openapi": "3.0.0",
    "info": {
        "title": "API Usuarios",
        "version": "1.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000"
        }
    ],
    "paths": {
        "/api/users": {
            "get": {
                "summary": "Mostrar usuarios",
                "operationId": "App\\Http\\Controllers\\AdministratorController::index",
                "responses": {
                    "200": {
                        "description": "Mostrar todos los usuarios."
                    },
                    "default": {
                        "description": "Ha ocurrido un error."
                    }
                }
            }
        },
        "/api/login": {
            "post": {
                "tags": [
                    "Autenticacion"
                ],
                "summary": "Autenticar usuario",
                "operationId": "App\\Http\\Controllers\\AuthController::login",
                "responses": {
                    "200": {
                        "description": "Mostrar Autenticacion"
                    },
                    "default": {
                        "description": "Ha ocurrido un error."
                    }
                }
            }
        }
    }
}